{
  "brain": {
    "type": "field",
    "sensory_dim": null,
    "motor_dim": null,
    "spatial_resolution": null,
    "features": {
      "enhanced_dynamics": true,
      "attention_guidance": true,
      "hierarchical_processing": true,
      "attention_super_resolution": false
    }
  },
  
  "memory": {
    "persistent_memory_path": "./robot_memory",
    "enable_persistence": true
  },
  
  "network": {
    "host": "0.0.0.0",
    "port": 9999,
    "monitoring_port": 9998
  },
  
  "logging": {
    "log_directory": "./logs",
    "log_level": "info",
    "enable_async_logging": true
  },
  
  "overrides": {
    "force_spatial_resolution": 4,
    "force_device": null,
    "disable_adaptation": false
  }
}